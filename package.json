{
  "name": "proyecto-tedw-covid19",
  "version": "1.0.0",
  "description": "Sistema de informacion para el control de casos de Covid-19 y consultas meicas en el ITC",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc-watch --onSuccess \"node build/index\"",
    "clean": "rm -rf build & rm -rf node_modules",
    "build": "tsc",
    "start": "node build",
    "migrations:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migrations:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "migrations:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate --name"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rate-limiter-flexible": "^2.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.3.0",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/redis": "^2.8.28",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4",
    "typeorm": "^0.2.32"
  }
}
